            "tsConfig": "libs/sms/tsconfig.spec.json",
            "karmaConfig": "libs/sms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/sms/tsconfig.lib.json",
              "libs/sms/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "admin": {
      "root": "libs/admin",
      "sourceRoot": "libs/admin/src",
      "projectType": "library",
      "prefix": "stats",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/admin/tsconfig.lib.json",
            "project": "libs/admin/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/admin/src/test.ts",
            "tsConfig": "libs/admin/tsconfig.spec.json",
            "karmaConfig": "libs/admin/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/admin/tsconfig.lib.json",
              "libs/admin/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "login": {
      "root": "libs/login",
      "sourceRoot": "libs/login/src",
      "projectType": "library",
      "prefix": "stats",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/login/tsconfig.lib.json",
            "project": "libs/login/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/login/src/test.ts",
            "tsConfig": "libs/login/tsconfig.spec.json",
            "karmaConfig": "libs/login/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/login/tsconfig.lib.json",
              "libs/login/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ptdb": {
      "root": "libs/ptdb",
      "sourceRoot": "libs/ptdb/src",
      "projectType": "library",
      "prefix": "stats",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/ptdb/tsconfig.lib.json",
            "project": "libs/ptdb/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/ptdb/src/test.ts",
            "tsConfig": "libs/ptdb/tsconfig.spec.json",
            "karmaConfig": "libs/ptdb/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ptdb/tsconfig.lib.json",
              "libs/ptdb/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "schematics": {},
  "defaultProject": "webapp"
}
